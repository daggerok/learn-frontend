{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "8.1.0",
    "bulma": "0.8.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "pm2 start 'npm start' --name app",
    "restart": "pm2 restart app",
    "stop": "pm2 kill",
    "logs": "pm2 logs",
    "gh": "cross-env PUBLIC_URL='/learn-frontend/' npm run build",
    "postgh": "npm-run-all postgh:*",
    "postgh:nojekyll": "ncp ./build/index.html ./build/.nojekyll",
    "postgh:404": "ncp ./build/index.html ./build/404.html"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "cross-env": "7.0.0",
    "ncp": "2.0.0",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "pm2": "4.2.3"
  }
}
